REM Greatest common divisor

INPUT "A=" TO ain
INPUT "B=" TO bin
a = VAL(ain)
b = VAL(bin)

WHILE b > 0
    t = a MOD b
    a = b
    b = t
ENDWHILE

PRINT "GCD=" + a

test = "ceci est un test"
PRINT UPPER(test)
PRINT LEFT(test,4)
PRINT SUBSTR(test,5,8)
PRINT RIGHT(test,4)
PRINT STARTSWITH(test,"ceci")
